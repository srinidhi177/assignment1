#include <stdio.h>
int main() {
    int n = 5; //aquare size
    int i, j;
    for (i = 1; i <= n; i++) {
        printf("*");
    }
    printf("\n");
    //middle rows forloop
    for (i = 2; i < n; i++) {
        printf("*");
        for (j = 2; j < n; j++) {
            printf(" ");
        }
        printf("*\n");
    }
    // bottom border
    for (i = 1; i <= n; i++) {
        printf("*");
    }
    printf("\n");
    return 0;
}
